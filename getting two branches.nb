Cell[TextData[{
 "We use dRGT Lagrangian for ghost-free massive gravity\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["L", "TI"], "\[LongEqual]", 
         FractionBox[
          SubsuperscriptBox[
           StyleBox["M", "TI"], 
           RowBox[{
             StyleBox["P", "TI"], 
             StyleBox["l", "TI"]}], "2"], "2"], 
         SqrtBox[
          RowBox[{"-", 
            StyleBox["g", "TI"]}]], 
         RowBox[{"(", 
           RowBox[{
             StyleBox["R", "TI"], "-", "2", 
             SuperscriptBox[
              StyleBox["m", "TI"], "2"], 
             UnderoverscriptBox["\[Sum]", 
              RowBox[{
                StyleBox["n", "TI"], "\[LongEqual]", "0"}], "4", 
              LimitsPositioning -> True], 
             SubscriptBox["\[Beta]", 
              StyleBox["n", "TI"]], 
             SubscriptBox[
              StyleBox["S", "TI"], 
              StyleBox["n", "TI"]], 
             RowBox[{"(", 
               StyleBox["X", FontSlant -> "Plain"], ")"}]}], ")"}], 
         "\[LongEqual]", 
         FractionBox[
          SubsuperscriptBox[
           StyleBox["M", "TI"], 
           RowBox[{
             StyleBox["P", "TI"], 
             StyleBox["l", "TI"]}], "2"], "2"], 
         SqrtBox[
          RowBox[{"-", 
            StyleBox["g", "TI"]}]], 
         RowBox[{"(", 
           RowBox[{
             StyleBox["R", "TI"], "-", "2", 
             SuperscriptBox[
              StyleBox["m", "TI"], "2"], 
             SubscriptBox[
              StyleBox["L", "TI"], 
              StyleBox["m", "TI"]]}], ")"}], ",", 
         SubscriptBox[
          StyleBox["L", "TI"], 
          StyleBox["m", "TI"]], "\[LongEqual]", 
         UnderoverscriptBox["\[Sum]", 
          RowBox[{
            StyleBox["n", "TI"], "\[LongEqual]", "0"}], "4", 
          LimitsPositioning -> True], 
         SubscriptBox["\[Beta]", 
          StyleBox["n", "TI"]], 
         SubscriptBox[
          StyleBox["S", "TI"], 
          StyleBox["n", "TI"]], 
         RowBox[{"(", 
           StyleBox["X", "TI"], ")"}]}], TraditionalForm], "errors" -> {}, 
     "input" -> 
     "\\mathcal{L} = \\frac{M_{Pl}^2}{2} \\sqrt{-g} \\left( R - 2m^2 \
\\sum_{n=0}^4 \\beta_n S_n(\\mathbb{X}) \\right) = \\frac{M_{Pl}^2}{2} \
\\sqrt{-g} \\left( R - 2m^2 L_m), L_m = \\sum_{n=0}^4 \\beta_n S_n (X)", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "4474e067-9187-4449-acaf-7f35f210e4f6"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["S", "TI"], "0"], 
         RowBox[{"(", 
           StyleBox["X", "TI"], ")"}], "\[LongEqual]", "1"}], 
       TraditionalForm], "errors" -> {}, "input" -> "S_0(X) = 1", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "e3b691a6-1a53-46fa-a2fb-0aa0d77fac70"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["S", "TI"], "1"], 
         RowBox[{"(", 
           StyleBox["X", "TI"], ")"}], "\[LongEqual]", 
         RowBox[{"[", 
           StyleBox["X", "TI"], "]"}]}], TraditionalForm], "errors" -> {}, 
     "input" -> "S_1(X) = [X]", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "40649c64-288b-4fa2-9eb9-b9d47d2fe6de"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["S", "TI"], "2"], 
         RowBox[{"(", 
           StyleBox["X", "TI"], ")"}], "\[LongEqual]", 
         FractionBox["1", "2"], 
         RowBox[{"(", 
           RowBox[{
             SuperscriptBox[
              RowBox[{"[", 
                StyleBox["X", "TI"], "]"}], "2"], "-", 
             RowBox[{"[", 
               SuperscriptBox[
                StyleBox["X", "TI"], "2"], "]"}]}], ")"}]}], TraditionalForm],
      "errors" -> {}, "input" -> "S_2(X) = \\frac{1}{2} ([X]^2 - [X^2])", 
     "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "e66e2f37-22ac-442c-983c-407d8ad7e772"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["S", "TI"], "3"], 
         RowBox[{"(", 
           StyleBox["X", "TI"], ")"}], "\[LongEqual]", 
         FractionBox["1", "6"], 
         RowBox[{"(", 
           RowBox[{
             SuperscriptBox[
              RowBox[{"[", 
                StyleBox["X", "TI"], "]"}], "3"], "-", "3", 
             RowBox[{"[", 
               StyleBox["X", "TI"], "]"}], 
             RowBox[{"[", 
               SuperscriptBox[
                StyleBox["X", "TI"], "2"], "]"}], "+", "2", 
             RowBox[{"[", 
               SuperscriptBox[
                StyleBox["X", "TI"], "3"], "]"}]}], ")"}]}], TraditionalForm],
      "errors" -> {}, "input" -> 
     "S_3 (X) = \\frac{1}{6} ([X]^3 - 3[X] [X^2] + 2[X^3])", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "cb4dbd81-44a6-4f3c-96b9-1c53ae4fa34b"],
 "\nwhere we assume ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox["\[Beta]", "4"], "\[LongEqual]", "0"}], 
       TraditionalForm], "errors" -> {}, "input" -> "\\beta_4 = 0", "state" -> 
     "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "470d89d5-535a-4f11-a437-cb5c9704cc36"],
 " and take ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         StyleBox["X", "TI"], "\[LongEqual]", 
         SqrtBox[
          RowBox[{
            SuperscriptBox[
             StyleBox["g", "TI"], "-1"], 
            StyleBox["f", "TI"]}]]}], TraditionalForm], "errors" -> {}, 
     "input" -> "X = \\sqrt{g^{-1} f}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "20be5ce5-3f8c-445b-a92a-fb70070412a1"],
 " symbolically.\n\nVarying with respect to ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       SuperscriptBox[
        StyleBox["g", "TI"], "\[Mu]\[Nu]"], TraditionalForm], "errors" -> {}, 
     "input" -> "g^{\\mu\\nu}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "4a703e27-6026-4f6f-b196-84466be7dad4"],
 " gives\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["G", "TI"], "\[Mu]\[Nu]"], "+", 
         SuperscriptBox[
          StyleBox["m", "TI"], "2"], 
         SubscriptBox[
          StyleBox["T", "TI"], "\[Mu]\[Nu]"], "\[LongEqual]", "0"}], 
       TraditionalForm], "errors" -> {}, "input" -> 
     "G_{\\mu\\nu} + m^2 T_{\\mu\\nu} = 0", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "5659cae3-0683-41c4-8eb8-60f161c21545"],
 "\n",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{
         SubscriptBox[
          StyleBox["T", "TI"], "\[Mu]\[Nu]"], "\[LongEqual]", 
         SubscriptBox[
          StyleBox["g", "TI"], "\[Mu]\[Nu]"], 
         SubscriptBox[
          StyleBox["L", "TI"], 
          StyleBox["m", "TI"]], "-", "2", 
         FractionBox[
          RowBox[{"\[Delta]", 
            SubscriptBox[
             StyleBox["L", "TI"], 
             StyleBox["m", "TI"]]}], 
          RowBox[{"\[Delta]", 
            SuperscriptBox[
             StyleBox["g", "TI"], "\[Mu]\[Nu]"]}]]}], TraditionalForm], 
     "errors" -> {}, "input" -> 
     "T_{\\mu\\nu} = g_{\\mu\\nu} L_m - 2 \\frac{\\delta L_m}{\\delta g^{\\mu\
\\nu}}", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "ac10e1fb-a5be-44f6-9494-f1dfee7767c5"],
 "\nFirst calculate the matrix X, and lowering the matrix for X"
